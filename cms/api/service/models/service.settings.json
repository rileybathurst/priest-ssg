{
  "connection": "default",
  "collectionName": "services",
  "info": {
    "name": "Service"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "Title": {
      "type": "string"
    },
    "Content": {
      "type": "richtext"
    },
    "Cover": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "services"
    },
    "Excerpt": {
      "type": "text"
    },
    "Gallery": {
      "collection": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    }
  }
}
